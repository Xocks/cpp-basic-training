#include <iostream>
#include <string>
using namespace std;
int main()
{
  string num;
  cin >>num;
  while (num.length()<6)
  {
   num= "0"+ num;//给字符型加补到6位数
  }
  int qian = (num[0]-'0')+(num[1]-'0')+(num[2]-'0');
//减‘0’
//字符'0'的ASCII码是48，那么'0' - '0' 就是 48 - 48 = 0。
//字符'1'的ASCII码是49，那么'1' - '0' 就是 49 - 48 = 1。
  int ho =(num[3]-'0')+(num[4]-'0')+(num[5]-'0');
  if (qian == ho)
  {
    cout<<"YES";
  }
  else 
  {
cout << "NO";

  }
  return 0;
}
